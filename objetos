let amigo = {nome: 'Tiago',
 sexo: 'M',
  peso: 85.2,
   engordar(p=0){
        console.log('Engordou')
        this.peso += p
   }}
   amigo.engordar()
   console.log(`${amigo.nome} peso ${amigo.peso}kg`)

